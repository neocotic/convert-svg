  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "tshy",
    "test": "node --import tsx --enable-source-maps --test \"test/**/*.test.ts\""
  },
  "dependencies": {
    "convert-svg-core": "^0.7.0",
    "del": "^8.0.0",
